{
  "name": "bitkjs",
  "version": "0.1.0",
  "description": "Bioinformatics Toolkit in Javascript (Node.js)",
  "scripts": {
    "test": "NODE_PATH=node_modules node_modules/.bin/nyc node_modules/.bin/mocha --reporter mocha-pretty-bunyan-nyan $(find src -name '*tests.js')",
    "mocha": "NODE_PATH=node_modules node_modules/.bin/mocha --reporter mocha-pretty-bunyan-nyan $(find src -name '*tests.js')",
    "coverage": "NODE_PATH=node_modules node_modules/.bin/nyc report --reporter=text-lcov | coveralls",
    "eslint": "node_modules/.bin/eslint src/*.js src/core/*js bin/*.js; exit 0",
    "eslint-pc": "node_modules/.bin/eslint src/*.js src/core/*js bin/*.js",
    "lint": "jshint .",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daviortega/bitkJS.git"
  },
  "keywords": [
    "bioinformatics"
  ],
  "author": "Davi Ortega",
  "contributors": [
    {
      "name": "Davi Ortega"
    }
  ],
  "pre-commit": [
    "eslint-pc"
  ],
  "bin": {
    "bitkJS.longHeader": "./bin/replaceHeader.js",
    "bitkJS.phypro.rpsjson2csv": "./bin/phypro.rpsjson2csv.js",
    "bitkJS.parseBlastResults": "./bin/parseBlastResults.js",
    "bitkJS.bitk2Tobitk3": "./bin/bitk2Tobitk3.js",
    "bitkJS.SortByTag": "./bin/SortByTag.js",
    "bitkJS.excludeFrom2ndFasta": "./bin/excludeFrom2ndFasta.js",
    "bitkJS.getFasta": "./bin/getFastaFromStableIds-cli.js",
    "bitkJS.getTaxSumm4fasta": "./bin/getTaxonomySummary4fasta-cli.js"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daviortega/bitkJS/issues"
  },
  "homepage": "https://github.com/daviortega/bitkJS#readme",
  "dependencies": {
    "JSONStream": "^1.3.1",
    "argparse": "^1.0.9",
    "bionode-fasta": "^0.5.6",
    "bluebird": "^3.5.0",
    "bunyan": "^1.8.8",
    "bunyan-prettystream": "^0.1.3",
    "chalk": "^2.3.2",
    "core-bio": "0.0.7",
    "csv-parse": "^1.2.0",
    "figlet": "^1.2.0",
    "mocha-pretty-bunyan-nyan": "^1.0.4",
    "node-mist3": "^1.5.3",
    "nyc": "^13.1.0",
    "pumpify": "^1.3.5",
    "sort-stream": "^1.0.1",
    "text2json": "^2.2.2"
  },
  "private": true,
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^2.11.15",
    "eslint": "^3.14.1",
    "eslint-config-google": "^0.7.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "pre-commit": "^1.2.2",
    "precommit-hook": "^3.0.0",
    "sinon": "^1.17.7"
  }
}
